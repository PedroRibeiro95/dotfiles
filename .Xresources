!! URxvt.font: xft:Source Code Pro:size=10.5
!! URxvt.depth:                      32
!! URxvt*scrollBar:                  false
!! URxvt*mouseWheelScrollPage:       false
!! URxvt*cursorBlink:                true
!! URxvt*saveLines:                  5000
!! URxvt*internalBorder: 5
!! URxvt*geometry: 70x19

rofi.color-enabled: true
rofi.color-window: #3b3024, #ba452f, #ba452f
rofi.color-normal: #3b3024, #cba96e, #3b3024, #3b3024, #cf4a39
rofi.color-active: #3b3024, #cba96e, #3b3024, #3b3024, #cf4a39
rofi.color-urgent: #3b3024, #cba96e, #3b3024, #3b3024, #cf4a39

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro Semibold 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

Xft.dpi: 144 
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!! URxvt Appearance
!! URxvt.font: xft:Source Code Pro:style=Regular:size=15 
!! URxvt.boldFont: xft:Source Code Pro:style=Bold:size=15
!! URxvt.italicFont: xft:Source Code Pro:style=Italic:size=15
!! URxvt.boldItalicfont: xft:Fira sans:style=Bold Italic:size=15
URxvt*font: xft:Source Code Pro Medium:size=16
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false
