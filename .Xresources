rofi.color-enabled: true
rofi.color-window: #282c35, #cbb8b9, #cbb8b9
rofi.color-normal: #282c35, #cbb8b9, #282c35, #282c35, #e29ec5
rofi.color-active: #282c35, #cbb8b9, #282c35, #282c35, #e29ec5
rofi.color-urgent: #282c35, #cbb8b9, #282c35, #282c35, #e29ec5

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro Semibold 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.dpi: 144

Xcursor.size: 32

Xft.dpi: 192 
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!! URxvt Appearance
!! URxvt.font: xft:Source Code Pro:style=Regular:size=15 
